pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
p1 = {}
gravity = 0.5

p1_walk_right={32,3,33,3,34,3,33,3}
p1_walk_left={48,3,49,3,50,3,49,3}
p1_stand={35,18,36,2,35,2,36,2,37,12,35,30,48,8,35,20,48,4,32,8}

function createentity()
 local e={x=0,y=0,vel={x=0,y=0},acc={x=0,y=0}}
 e.animation={0,1}
 e.animationidx=0
 e.animationdelay=0
 e.spr=0
 return e
end
function _init()
 p1=createentity()
 setanimation(p1,p1_stand)
 p1.x=50
 p1.y=50
end
function collisiontilemap(e)
 local x=e.x
 local y=e.y
 local p1={x=flr((x+1)/8),y=flr((y+0)/8)}
 local p2={x=flr((x+7)/8),y=flr((y+0)/8)}
 local p3={x=flr((x+1)/8),y=flr((y+7)/8)}
 local p4={x=flr((x+7)/8),y=flr((y+7)/8)}
 local collidingtiles={}
 if fget(mget(p1.x, p1.y),0) then add(collidingtiles, p1) end 
 if fget(mget(p2.x, p2.y),0) then add(collidingtiles, p2) end 
 if fget(mget(p3.x, p3.y),0) then add(collidingtiles, p3) end 
 if fget(mget(p4.x, p4.y),0) then add(collidingtiles, p4) end 
 return collidingtiles
end
debugmsg = ""
debugmsg2 = ""
function collisionresolve(e)
 -- reset entity pos
 local ctiles=collisiontilemap(e)
 local before={x=e.x,y=e.y,vel={x=e.vel.x, y=e.vel.y}}
 e.vel.x = before.vel.x
 e.vel.y = 0
 e.x = before.x
 e.y = before.y - before.vel.y
 if #collisiontilemap(e)==0 then
  if(before.vel.y<0) then e.y=(ctiles[1].y+1)*8 else e.y=(ctiles[1].y-1)*8 end 
  return 
 end
 e.vel.x = 0 
 e.vel.y = before.vel.y
 e.x = before.x - before.vel.x
 e.y = before.y
 if #collisiontilemap(e)==0 then
  return 
 end
 e.vel.x = 0
 e.vel.y = 0
 e.x = before.x - before.vel.x
 e.y = before.y - before.vel.y
end
function physics()
 p1.vel.x += p1.acc.x
 p1.vel.y += p1.acc.y
 p1.vel.y += gravity
 p1.x += p1.vel.x
 p1.y += p1.vel.y
end
function setanimation(e, animation)
if(e.animation == animation) then return end
 e.animation = animation
 e.animationidx = 0
 e.animationdelay = 0
end
function animate(e)
e.animationdelay -= 1
 if e.animationdelay < 0 then 
  e.animationidx += 1
  if e.animationidx > #e.animation then 
   e.animationidx = 1
  end
  e.spr = e.animation[e.animationidx]
  e.animationdelay = e.animation[e.animationidx+1]
  e.animationidx += 1
 end
end
function _update()
 local speed = 1.5
 p1.vel.x = 0
 p1.acc.y = 0
 if btn(0) then p1.vel.x -= speed; setanimation(p1,p1_walk_left) end
 if btn(1) then p1.vel.x += speed; setanimation(p1,p1_walk_right) end
 if btnp(2) then p1.acc.y = -3*speed end
 if p1.vel.x==0 then  
  setanimation(p1,p1_stand)
 end
 physics(p1)
 animate(p1)

 if #collisiontilemap(p1)>0 then 
  collisionresolve(p1)
 end
end
function _draw()
 cls()
 map(0,0,0,0,16,16)
 spr(p1.spr,flr(p1.x),flr(p1.y))
 print(debugmsg,0,0)
 print(debugmsg2,0,10)
end
